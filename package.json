    "pre-commit": "1.2.2",
    "shelljs": "^0.8.2",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "svg-url-loader": "2.3.2",
    "url-loader": "1.0.1",
    "webpack-cli": "3.0.8",
    "webpack-pwa-manifest": "3.6.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "linebreak-style": 0,
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "react/jsx-filename-extension": 0,
      "react/sort-comp": 0
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "build/"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
